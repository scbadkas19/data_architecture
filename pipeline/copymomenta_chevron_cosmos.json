{
	"name": "copymomenta_chevron_cosmos",
	"properties": {
		"description": "Copying chevron Momenta data to cosmos db - ",
		"activities": [
			{
				"name": "CopyChevron From MomnetaMongoDB",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "users"
					},
					{
						"name": "Destination",
						"value": "Chevron"
					}
				],
				"typeProperties": {
					"source": {
						"type": "MongoDbV2Source",
						"batchSize": 100,
						"filter": "",
						"cursorMethods": {
							"project": "{\"_id\":1,\"lastSuccessfulLoginAt\":1}"
						}
					},
					"sink": {
						"type": "CosmosDbMongoDbApiSink",
						"writeBatchTimeout": "00:30:00",
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_h8n",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_h8n",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "CopyChevron From SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT distinct HASHBYTES('MD5', [Name]) as hashedname\n      ,[UpdatedOn]\n      \n  FROM [syntase].[dbo].[OnlineUser]",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "CosmosDbMongoDbApiSink",
						"writeBatchTimeout": "00:30:00",
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "hashedname",
									"type": "Byte[]"
								},
								"sink": {
									"path": "$['_id']['$oid']"
								}
							},
							{
								"source": {
									"name": "UpdatedOn",
									"type": "DateTime"
								},
								"sink": {
									"path": "$['lastSuccessfulLoginAt']['$date']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SqlServerTable1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_h8n",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2021-06-21T08:26:51Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}